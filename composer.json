{
  "name": "miaoxing/logistics",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Logistics Plugin",
  "keywords": [
    "miaoxing",
    "logistics"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/admin": "^0.9.1",
    "miaoxing/app": "^0.6.8",
    "miaoxing/region": "^0.2.11"
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.3"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Logistics\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Logistics\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.9.x-dev",
      "miaoxing/services": "miaoxing/services as 0.6.x-dev",
      "miaoxing/admin": "miaoxing/admin as 0.9.x-dev",
      "wei/wei": "twinh/wei as 0.14.x-dev"
    }
  }
}
